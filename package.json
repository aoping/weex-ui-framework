{
  "name": "weex-ui-framework",
  "version": "1.0.0",
  "description": "weex ui framework",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build-prod": "cross-env NODE_ENV=prod webpack",
    "build_plugin": "webpack --config ./tools/webpack.config.plugin.js --color",
    "debug": "weex-devtool",
    "dev": "webpack --watch",
    "serve": "webpack-dev-server --config webpack.dev.js -p --open --hot --inline",
    "serve2": "serve -p 8081",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "init": "node build/init.js",
    "init:local": "node build/init.js local",
    "copy:android": "shx rm -rf ./platforms/android/app/src/main/assets/* && shx cp -r ./dist/* ./platforms/android/app/src/main/assets/",
    "copy:ios": "shx rm -rf ./platforms/ios/bundlejs/* && shx cp -r ./dist/* ./platforms/ios/bundlejs/",
    "copy": "npm run copy:android && npm run copy:ios",
    "prod": "cross-env NODE_ENV=prod webpack"
  },
  "keywords": [
    "weex",
    "vue"
  ],
  "dependencies": {
    "fs-extra": "^4.0.2",
    "he": "^1.1.1",
    "js-base64": "^2.4.0",
    "lodash": "^4.17.4",
    "object-assign": "^4.1.0",
    "qs": "^6.5.1",
    "vue": "~2.3.4",
    "vue-router": "^2.7.0",
    "weex-html5": "^0.4.1",
    "weex-vue-render": "^0.12.25"
  },
  "devDependencies": {
    "@weex-project/weex-picker": "^0.2.4",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-component": "^0.10.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^3.1.0",
    "eslint-plugin-import": "^2.2.0",
    "file-loader": "^0.11.2",
    "inject-loader": "^3.0.0",
    "ip": "^1.1.5",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "lolex": "^1.5.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.14",
    "quick-local-ip": "^1.0.7",
    "sass-loader": "^6.0.6",
    "serve": "^6.0.2",
    "shx": "^0.2.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "vue-hot-reload-api": "^2.1.0",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "~2.3.4",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "weex-builder": "^0.2.6",
    "weex-devtool": "^0.2.82",
    "weex-loader": "^0.4.5",
    "yaml-loader": "~0.4.0"
  },
  "optionalDependencies": {
    "ios-deploy": "^1.9.1"
  }
}
